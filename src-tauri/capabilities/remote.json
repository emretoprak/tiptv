{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "remote",
  "description": "enables permissions for remote URL",
  "windows": [
    "main"
  ],
  "webviews": [
    "main"
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows",
    "android",
    "iOS"
  ],
  "remote": {
    "urls": [
      "https://topraktv-dc925.web.app",
      "https://topraktv-dc925.web.app/*",
      "https://topraktv-dc925.firebaseapp.com",
      "https://topraktv-dc925.firebaseapp.com/*"
    ]
  },
  "permissions": [
    "core:default",
    "core:window:default",
    "core:webview:default",
    "core:webview:allow-print",
    "core:webview:allow-create-webview",
    "core:webview:allow-create-webview-window",
    "core:webview:allow-internal-toggle-devtools",
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-load",
    "store:allow-save",
    "store:allow-delete",
    "store:allow-clear",
    "store:allow-keys",
    "store:allow-values",
    "store:allow-entries",
    "store:allow-length",
    "store:allow-has",
    "store:allow-reset"
  ]
}
